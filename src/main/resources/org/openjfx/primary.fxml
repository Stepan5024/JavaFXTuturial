<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<VBox alignment="CENTER" focusTraversable="true" prefHeight="380.0" prefWidth="499.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.PrimaryController">
   <children>
      <Label alignment="CENTER_RIGHT" text="Primary View">
         <opaqueInsets>
            <Insets bottom="1.0" left="1.0" right="2.0" top="5.0" />
         </opaqueInsets>
         <effect>
            <Lighting bumpInput="$null">
               <light>
                  <Light.Distant>
                     <color>
                        <Color red="0.6657894849777222" green="0.3341276943683624" blue="0.3341276943683624" />
                     </color>
                  </Light.Distant>
               </light>
            </Lighting>
         </effect></Label>
      <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Switch to Secondary View" />
      <ColorPicker />
      <CheckBox mnemonicParsing="false" text="CheckBox" />
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0" />
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
